
<% function check_structure(el) {
  if (el !== '' && typeof el !== 'undefined' && el !== '[{}]' && el !== '[]') {
    return true
  } return false
} %>
<% try  {    if (check_structure(d.layouts)) { %>
  <div class="single_tab" id="single_tab3">
      <ul class=" single_navigation nav nav-pills">
        <li><a  href="#single_tab">Описание</a></li>
        <li><a  href="#single_tab1">Цены</a></li>
        <li><a  href="#single_tab2">Рассрочка</a></li>
        <%  if (check_structure(d.layouts)) { %>
        <li><a class="tab_active"  href="#single_tab3">Планировки</a></li>
        <%  } %>
      </ul>
      <div id="menu3" class="tab-pane fade">
<div class="planning_tab">
<div class="panel_col flex_col">
<div class="panel collapsable closed">
  <h4 class="underlined">Планировки</h4>
<ul class="nav nav-pills" >
  <% i = 0; JSON.parse(d.layouts).forEach(elem => {  %>
  <%  i ++; let active, active1; if (i == 1) { active = 'in active'; activelink = 'tab_active'; } else { active = activelink = ''}  ;   %>
  <%   if (elem.text !== undefined) { %>
     <li class=" <%= active %>" ><a data-toggle="pill" class=" <%= activelink %>" href="#stab_<%= i %>"><span class="big circle"></span><%= elem.text %></a></li>
  <%  }}); %>
</ul>
  <a id="show_all" class="show_all">Смотреть все <i class="fa fa-arrow-down"></i></a>
</div>
<div class="button_c">
      <button type="button" name="button">Наличие квартир</button>
      <span class="phone_icon"><img src="/images/phone.png"></span>
    </div>
    </div>
<div class="tab-content" id="lgallery">
  <% i = 0; JSON.parse(d.layouts).forEach(elem => { i ++; let active; if (i == 1) { active = 'in active' } else { active = ''};if (elem.media !== undefined && elem.media !== '') {  %>
      <div id="stab_<%= i %>" class="tab-pane fade <%= active %>"> <div class="plannimg"> <img src="/uploads/<%= elem.media %>" /> </div> </div>
  <%  } }); %>
</div>
</div>
</div>
</div>
<script type="text/javascript">
  $('a#show_all').click(function(){
  $('.panel.collapsable').toggleClass('closed')
  })
</script>







<link rel="stylesheet" href="https://cdn.rawgit.com/sachinchoolur/lightgallery.js/master/dist/css/lightgallery.css">
<script src="https://cdn.rawgit.com/sachinchoolur/lightgallery.js/master/dist/js/lightgallery.js"></script>
<script>
var parent = '#lgallery';
/* single mode , you must make a multi(slider) mode*/

function make_light(parent) {
  jQuery(''+ parent +' img').each(function(){ var src = jQuery(this).attr('src'); jQuery(this).wrap('<p><a class="item" href="' + src + '"></a></p>') }); setTimeout(function(){ document.querySelectorAll(''+ parent +' p').forEach(function(element) { lightGallery(element); }); }, 10)
}

make_light(parent)

</script>












<% }  }     catch (e) { console.log(e);}  %>
