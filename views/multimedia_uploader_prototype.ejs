<% function print_files_object(msg){
    result = ''
    if (typeof msg != 'undefined')
    JSON.parse(msg).forEach(function(file){
       let link = '/uploads/' + file.filename
       result += '<div class="gallery_img"> <img src="' + link + '"></div>'
    });
    return result
    }
    %>
<% function print_filenames(msg){
    result = []
    if (typeof msg != 'undefined')
    JSON.parse(msg).forEach(function(file){
      result.unshift(file.filename)
    });
    return result.join()
    }
    %>



