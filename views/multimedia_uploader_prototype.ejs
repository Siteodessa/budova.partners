        <div class="btn">
          <span><i class="fa fa-camera"></i></span>
          <input name="gallery" id="gallery" type="file" multiple>
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate" type="text">
        </div>
      </div>
    </form>

  </div>

